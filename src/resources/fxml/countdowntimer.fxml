<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controllers.CountdownTimerController" fx:id="countdownTimer">
    <children>
        <TextField fx:id="timerViewer" alignment="CENTER" editable="false" prefHeight="81.0" prefWidth="295.0" text="00 : 00 : 00">
            <font>
                <Font size="24.0" />
            </font>
        </TextField>
        <Button fx:id="buttonStartPauseTimer" mnemonicParsing="false" onMouseClicked="#timerStartPause" prefHeight="80.0" prefWidth="258.0" text="Start" />
        <Button fx:id="buttonResetTimer" mnemonicParsing="false" onMouseClicked="#timerReset" prefHeight="80.0" prefWidth="269.0" text="Reset" />
    </children>
</HBox>